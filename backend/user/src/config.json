{
    "helmet": {
        "contentSecurityPolicy": {
            "directives": {
                "defaultSrc": ["'self'"],
                "scriptSrc": ["'self'"],
                "styleSrc": ["'self'"],
                "imgSrc": ["'self'"],
                "fontSrc": ["'self'"],
                "connectSrc": ["'self'"],
                "objectSrc": ["'none'"],
                "mediaSrc": ["'none'"],
                "frameSrc": ["'none'"],
                "upgradeInsecureRequests": []
            }
        },
        "hsts": {
            "maxAge": 31536000,
            "includeSubDomains": true,
            "preload": true
        },
        "referrerPolicy": {
            "policy": "no-referrer"
        },
        "frameguard": {
            "action": "deny"
        }
    }
}